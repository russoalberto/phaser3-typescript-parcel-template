{
  "name": "phaser3-typescript-parcel-template",
  "version": "1.0.0",
  "author": "russoalberto",
  "description": "A typescript template project for Phaser 3 using Parceljs 2",
  "homepage": "https://github.com/russoalberto/phaser3-typescript-parcel-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/russoalberto/phaser3-typescript-parcel-template.git"
